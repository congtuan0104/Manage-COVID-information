<script>
    $(document).ready(function () {
        $('#{{{currentPage}}}').addClass('active');
    });

    document.querySelector(".inputNumber").addEventListener("keypress", function (evt) {
        if (evt.which != 8 && evt.which != 0 && evt.which < 48 || evt.which > 57) {
            evt.preventDefault();
        }
    });
</script>